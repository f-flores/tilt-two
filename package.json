{
  "name": "tilt",
  "version": "1.0.0",
  "description": "track and report online gaming cheaters",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SigmaVX/Tilt.git"
  },
  "author": "CloudMash LLC - Tony Wible",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SigmaVX/Tilt/issues"
  },
  "homepage": "https://github.com/SigmaVX/Tilt#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.0",
    "connect-mongo": "^2.0.1",
    "create-react-app": "^1.5.2",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "google-auth-library": "^1.6.1",
    "googleapis": "^32.0.0",
    "if-env": "^1.0.4",
    "moment": "^2.22.2",
    "mongoose": "^5.1.6",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.5",
    "socket.io": "^2.1.1"
  }
}
